{
  "name": "husky-standard-version-playground",
  "version": "1.0.3",
  "description": "Just a playground",
  "main": "index.js",
  "scripts": {
    "test": "./gradlew build",
    "prepare": "husky install",
    "release": "./gradlew build && standard-version",
    "release:minor": "./gradlew build && standard-version --release-as minor",
    "release:patch": "./gradlew build && standard-version --release-as patch",
    "release:major": "./gradlew build && standard-version --release-as major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TechIsFun/husky-standard-version-playground.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TechIsFun/husky-standard-version-playground/issues"
  },
  "homepage": "https://github.com/TechIsFun/husky-standard-version-playground#readme",
  "devDependencies": {
    "husky": "7.0.1",
    "standard-version": "9.3.1"
  },
  "dependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0"
  }
}
